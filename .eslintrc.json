{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {},
  "import/resolver": {
    "node": {
      "extensions": [".js", ".ts"],
      "moduleDirectory": ["node_modules", "src/"]
    }
  }
}
