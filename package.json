{
	"name": "express_userapp",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "swc src -d dist",
		"start": "pnpm run build && node dist/index.js",
		"build-dev": "pnpm run build && cross-env NODE_ENV=development node --inspect dist/index.js",
		"dev": "nodemon --ext ts --exec \"pnpm run build-dev\"",
		"compile-schemas": "json2ts -i schemas -o types",
		"migrate:make": "npx ts-node ./migrate.ts make:migration --name",
		"migrate:up": "npx knex migrate:latest --migrations-directory ./migrations --env=development",
		"migrate:down": "npx knex migrate:down --migrations-directory ./migrations --env=development",
		"migrate:rollback": "npx knex migrate:rollback --migrations-directory ./migrations --env=development"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"ajv": "^8.12.0",
		"bcrypt": "^5.1.1",
		"bluebird": "^3.7.2",
		"dayjs": "^1.11.10",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"knex": "^3.0.1",
		"mysql2": "^3.6.3",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.96",
		"@swc/register": "^0.1.10",
		"@types/bcrypt": "^5.0.2",
		"@types/bluebird": "^3.5.42",
		"@types/express": "^4.17.21",
		"@types/node": "^20.9.0",
		"@types/yargs": "^17.0.31",
		"cross-env": "^7.0.3",
		"json-schema-to-typescript": "^13.1.1",
		"nodemon": "^3.0.1",
		"sequelize-cli": "^6.6.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	}
}
